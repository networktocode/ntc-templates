  ^.*RRM Neighbor Disc\w+ Type\.*\s${NEIGH_DISCOVER_TYPE}s*$$
  ^.*RRM Neighbor RSSI Normalization\.*\s${NEIGH_RSSI_NORM}s*$$
  ^.*AP Coverage Interval\.*\s${COVERAGE_INTERVAL}s*$$
  ^.*AP Load Interval\.*\s${LOAD_INTERVAL}s*$$
  ^.*AP Monitor Measurement Interval\.*\s${MON_MEASURE_INTERVAL}s*$$
