config system interface
    edit "wan"
        set vdom "root"
        set ip 1.2.3.4 255.255.255.252
        set allowaccess ping https ssh snmp http
        set type physical
        set lldp-reception enable
        set monitor-bandwidth enable
        set role wan
        set snmp-index 1
        set secondary-IP enable
        config secondaryip
            edit 1
                set ip 10.15.76.2 255.255.255.248
                set allowaccess ping
            next
        end
    next
    edit "a"
        set vdom "root"
        set status down
        set type physical
        set snmp-index 5
    next
    edit "modem"
        set vdom "root"
        set mode pppoe
        set type physical
        set snmp-index 2
    next
    edit "ssl.root"
        set vdom "root"
        set type tunnel
        set alias "SSL VPN interface"
        set snmp-index 3
    next
    edit "lan"
        set vdom "root"
        set ip 10.152.1.230 255.255.255.252
        set allowaccess ping https ssh snmp http
        set type hard-switch
        set alias "LAN"
        set stp enable
        set device-identification enable
        set lldp-transmission enable
        set monitor-bandwidth enable
        set role lan
        set snmp-index 4
    next
    edit "Vlan70"
        set vdom "root"
        set ip 10.18.123.193 255.255.255.224
        set allowaccess ping
        set alias "MGMT"
        set device-identification enable
        set monitor-bandwidth enable
        set role lan
        set snmp-index 12
        set interface "lan"
        set vlanid 70
    next
    edit "Loopback0"
        set vdom "root"
        set status down
        set type loopback
        set description "Virtual Interface for Config version control"
        set alias "C-21"
        set role lan
        set snmp-index 13
    next
    edit "Tu-Hub01-Main"
        set vdom "root"
        set ip 10.149.0.5 255.255.255.255
        set type tunnel
        set remote-ip 10.149.127.253 255.255.0.0
        set monitor-bandwidth enable
        set snmp-index 14
        set mtu-override enable
        set mtu 1300
        set interface "wan"
    next
end
